<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.organization.hr.activityhandel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gamelabel"
            android:label="@string/title_activity_gamelabel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PLAY1"
            android:label="@string/title_activity_play1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".play2"
            android:label="@string/title_activity_play2"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--
             ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".play3"
            android:label="@string/title_activity_play3"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Play4"
            android:label="@string/title_activity_play4"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Lose"
            android:label="@string/title_activity_lose"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
